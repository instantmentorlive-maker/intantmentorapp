# Copy to .env and fill in values
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Optional: Realtime WebSocket server
# Set to true to enable app-managed WebSocket connection
ENABLE_WEBSOCKET=false
# Provide when ENABLE_WEBSOCKET=true. Examples:
# WEBSOCKET_URL=http://localhost:3000
# WEBSOCKET_URL=wss://ws.yourdomain.com
WEBSOCKET_URL=

# API config (optional)
API_BASE_URL=https://api.instantmentor.com
API_VERSION=v1

# WebRTC (optional) - set ICE servers for NAT traversal
# Option 1: Provide JSON array (preferred):
# WEBRTC_ICE_SERVERS=[
#   {"urls":"stun:stun.l.google.com:19302"},
#   {"urls":"turn:turn.example.com:3478","username":"user","credential":"pass"}
# ]
WEBRTC_ICE_SERVERS=

# Option 2: Simple TURN variables (fallback if WEBRTC_ICE_SERVERS not set)
TURN_URL=
TURN_USERNAME=
TURN_CREDENTIAL=
